        <activity
            android:name=".view.activities.InfoActivity"
            android:label="@string/title_activity_info" >
        </activity>
        <activity
            android:name=".view.activities.ContentActivity"
            android:label="@string/title_activity_content" >
        </activity>
        <activity
            android:name=".view.activities.GalleryActivity"
            android:label="@string/title_activity_gallery" >
        </activity>
        <activity
            android:name=".view.activities.StaticMapActivity"
            android:label="@string/title_activity_static_map" >
        </activity>
        <activity
            android:name=".view.activities.StoryActivity"
            android:label="@string/title_activity_story" >
        </activity>
        <activity
            android:name=".view.activities.ProgressActivity"
            android:label="@string/title_activity_progress" >
        </activity>
        <activity
            android:name=".view.activities.RedirectActivity"
            android:label="@string/title_activity_redirect" >
        </activity>
        <activity
            android:name=".view.activities.ScanQRActivity"
            android:label="@string/title_activity_scan_qr" >
